<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <stroke android:color="@color/textColorLight" android:width="2dp"/>
            <corners android:radius="4dp"/>
            <size android:width="@dimen/cb_size" android:height="@dimen/cb_size"/>
        </shape>
    </item>
    <item android:top="@dimen/cb_padding_size" android:left="@dimen/cb_padding_size" android:width="@dimen/cb_inside_size" android:height="@dimen/cb_inside_size">
        <shape
            android:shape="rectangle">
            <corners android:radius="2dp"/>
            <solid android:color="@color/textColorLight"/>
        </shape>
    </item>
</layer-list>
